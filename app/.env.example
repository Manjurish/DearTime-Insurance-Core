# AWS SNS Configuration
AWS_SNS_KEY=your_sns_key_here
AWS_SNS_SECRET=your_sns_secret_here
AWS_SNS_REGION=ap-southeast-1

# Google Authentication
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT=your_redirect_url_here

# Facebook Authentication
FACEBOOK_CLIENT_ID=your_facebook_client_id_here
FACEBOOK_CLIENT_SECRET=your_facebook_client_secret_here
FACEBOOK_REDIRECT=your_redirect_url_here

# Screening Configuration
SCREENING_TOKEN=your_screening_token_here

# Application Version
APPLICATION_VERSION=0.1 